<!-- SSRFunctions -->
<!doctype html>
<html lang="fr">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, minimum-scale=1, initial-scale=1, user-scalable=yes">

  <!-- SSRHead -->

  <base href="/">

  <meta name="theme-color" content="#b47e71">

  <link rel="apple-touch-icon" sizes="180x180"
    href="<%= htmlWebpackPlugin.options.pathname %>/assets/icons/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32"
    href="<%= htmlWebpackPlugin.options.pathname %>/assets/icons/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16"
    href="<%= htmlWebpackPlugin.options.pathname %>/assets/icons/favicon-16x16.png">
  <link rel="manifest" href="<%= htmlWebpackPlugin.options.pathname %>/assets/site.webmanifest">
  <link rel="mask-icon" href="<%= htmlWebpackPlugin.options.pathname %>/assets/icons/safari-pinned-tab.svg"
    color="#5bbad5">
  <link rel="shortcut icon" href="<%= htmlWebpackPlugin.options.pathname %>/favicon.ico">
  <meta name="msapplication-TileColor" content="#ffffff">
  <meta name="msapplication-config" content="<%= htmlWebpackPlugin.options.pathname %>/assets/icons/browserconfig.xml">
  <meta name="theme-color" content="#ffffff">
  <link rel="preconnect" href="https://base.dobruniadesign.com" crossorigin>
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
  <link rel="preload" as="style" href="https://fonts.googleapis.com/css2?family=Material+Icons&display=swap" />
  <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Icons&display=swap" media="print"
    onload="this.media='all'" />
  <noscript>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Icons&display=swap" />
  </noscript>

  <!-- SSRPayload -->
   
  <noscript>JavaScript is needed to use this website.</noscript>
  <script src="<%= htmlWebpackPlugin.options.pathname %>/boot.js"></script>
</head>

<body>
  <elara-app></elara-app>
</body>
<style>
  @font-face {
    font-family: 'Hind';
    font-display: fallback;
    font-style: normal;
    font-weight: 400;
    src: local('Hind Regular'), local('Hind-Regular'),
      url('<%= htmlWebpackPlugin.options.pathname %>/assets/fonts/hind.woff2') format('woff2'),
      url('<%= htmlWebpackPlugin.options.pathname %>/assets/fonts/hind.woff') format('woff');
  }

  @font-face {
    font-family: 'Lora';
    font-display: fallback;
    font-style: normal;
    font-weight: 400;
    src: local('Lora Regular'), local('Lora-Regular'),
      url('<%= htmlWebpackPlugin.options.pathname %>/assets/fonts/lora.woff2') format('woff2'),
      url('<%= htmlWebpackPlugin.options.pathname %>/assets/fonts/lora.woff') format('woff');
  }

  @font-face {
    font-family: 'Kotori Rose';
    font-display: fallback;
    font-style: normal;
    font-weight: 400;
    src: local('Kotori Rose'), url('<%= htmlWebpackPlugin.options.pathname %>/assets/fonts/kotori-regular-webfont.woff2') format('woff2');
    unicode-range: U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6, U+02DA, U+02DC, U+2000-206F, U+2074, U+20AC, U+2122, U+2191, U+2193, U+2212, U+2215, U+FEFF, U+FFFD;
  }
</style>

</html>